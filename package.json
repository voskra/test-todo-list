    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.19",
    "postcss-css-variables": "^0.18.0",
    "postcss-import": "^15.0.1",
    "postcss-loader": "^7.0.2",
